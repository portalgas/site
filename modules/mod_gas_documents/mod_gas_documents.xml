<?xml version="1.0" encoding="utf-8"?>
<extension
	type="module"
	version="2.5"
	client="site"
	method="upgrade">
	<name>mod_gas_documents</name>
	<author>PortAlGas</author>
	<creationDate>October 2020</creationDate>
	<copyright></copyright>
	<license></license>
	<authorEmail>info@portalgas.it</authorEmail>
	<authorUrl>https://www.portalgas.it</authorUrl>
	<version>2.0.0</version>
	<description>Elenco documenti di un GAS</description>
	<files>
		<filename module="mod_gas_documents">mod_gas_documents.php</filename>
		<folder>tmpl</folder>
		<filename>helper.php</filename>
		<filename>index.html</filename>		
		<filename>mod_gas_documents.xml</filename>
	</files>
	<languages>
		<language tag="it-IT">it-IT.mod_gas_documents.ini</language>
		<language tag="it-IT">it-IT.mod_gas_documents.sys.ini</language>
	</languages>
	<help key="JHELP_EXTENSIONS_MODULE_MANAGER_GAS_DOCUMENTS" />
	<config>
		<fields name="params">	
			<fieldset
				name="advanced">
				<field
					name="layout"
					type="modulelayout"
					label="JFIELD_ALT_LAYOUT_LABEL"
					description="JFIELD_ALT_MODULE_LAYOUT_DESC" />
				<field
					name="moduleclass_sfx"
					type="text"
					label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
					description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
				<field
					name="cache"
					type="list"
					default="0"
					label="COM_MODULES_FIELD_CACHING_LABEL"
					description="COM_MODULES_FIELD_CACHING_DESC">
					<option
						value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>
				</field>
			</fieldset>
		</fields>
	</config>	
</extension>